{
  "css": {
    "exts": [".css"],
    "mimes": [
      "text/css"
    ]
  },
  "js": {
    "exts": [".js"],
    "mimes": [
      "application/javascript",
      "application/x-javascript",
      "application/ecmascript"
    ]
  },
  "img": {
    "exts": [".jpg", ".jpeg", ".png", ".gif"],
    "mimes": [
      "image/gif",
      "image/jpeg",
      "image/png"]
  },
  "fonts": {
    "exts": [".ttf", ".eot", ".woff", ".svg"],
    "mimes": [
      "application/x-font-ttf"
    ]
  }
}
